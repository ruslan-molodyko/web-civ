{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "watch": "ng build -op dist -w --base-href .",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "watch-server": "cd server && tsc -w",
    "test-server": "mocha tests dist-server/**/*.test.js --recursive --watch",
    "test-watch-server": "concurrently -k \"rmdir dist-server /s /q\" \"npm run watch-server\" \"npm run test-server\""
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "core-js": "^2.4.1",
    "nodejs-websocket": "^1.7.1",
    "rxjs": "^5.0.1",
    "ts-helpers": "^1.1.1",
    "ws": "^2.2.2",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.3.1",
    "@types/chai": "^3.4.35",
    "@types/jasmine": "2.5.38",
    "@types/node": "^7.0.8",
    "@types/sinon": "^1.16.36",
    "@types/ws": "0.0.39",
    "angular-cli": "1.0.0-beta.28.3",
    "chai": "^3.5.0",
    "codelyzer": "~2.0.0-beta.1",
    "concurrently": "^3.4.0",
    "electron": "^1.4.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "mocha": "^3.2.0",
    "protractor": "~4.0.13",
    "sinon": "^2.1.0",
    "ts-loader": "^2.0.3",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "~2.0.3"
  }
}
